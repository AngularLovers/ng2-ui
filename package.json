  "main": "dist/ng2-ui.umd.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "start": "http-server -o -p 9090",
    "clean": "rimraf dist",
    "build": "npm-run-all --serial clean build:ts build:umd",
